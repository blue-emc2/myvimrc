" Use deoplete.
let g:deoplete#enable_at_startup = 1

let g:python_host_prog = '/usr/local/bin/python2'
let g:python3_host_prog = '/usr/local/bin/python3'

set runtimepath+=~/myvimrc
runtime rc/dein.vim
runtime keymap.vim

set number
set expandtab
set tabstop=2
set shiftwidth=2
set t_Co=256¬
set termguicolors  " truecolor¬
set background=dark

colorscheme deep-space

syntax on

